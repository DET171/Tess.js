{
  "presets": [
    [
      "@babel/env",
      {
        "exclude": ["babel-plugin-transform-async-to-generator", "babel-plugin-transform-regenerator"]
      }
    ]
  ],
  "plugins": [
    [
      "module:fast-async",
      {
        "spec": true,
        "compiler": {
          "promises": true,
          "generators": false
        }
      }
    ]
  ]
}
